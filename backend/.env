# AWS Credentials (REPLACE WITH YOUR OWN)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1
AWS_S3_BUCKET=your_s3_bucket_name

# Database
DATABASE_URL=sqlite:///./cloudflux.db

# JWT Secret (CHANGE IN PRODUCTION)
JWT_SECRET_KEY=change_this_to_random_secret_key_in_production

# Demo mode OFF (use real cloud)
DEMO_MODE=False
USE_MOCK_DATA=False

# ====================================
# AZURE CREDENTIALS (REPLACE WITH YOUR OWN)
# ====================================
AZURE_STORAGE_ACCOUNT_NAME=your_azure_account_name
AZURE_STORAGE_ACCOUNT_KEY=your_azure_storage_key
AZURE_CONTAINER_NAME=your_container_name

# ====================================
# GCP CREDENTIALS (REPLACE WITH YOUR OWN)
# ====================================
GOOGLE_APPLICATION_CREDENTIALS=./gcp-credentials.json
GCP_PROJECT_ID=your_gcp_project_id
GCP_BUCKET_NAME=your_gcp_bucket_name
